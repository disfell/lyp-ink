{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.experimental.classRegex": [["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]],
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "ui", "active-class", "inactive-class"],
  "editor.fontSize": 11,
  "editor.fontFamily": "JetBrains Mono,sans-serif",
  "debug.console.fontSize": 10,
  "debug.console.fontFamily": "JetBrains Mono,Monaco,Cascadia Mono,Consolas,sans-serif",
  "terminal.integrated.fontFamily": "JetBrains Mono,Monaco,Cascadia Monosans-serif,Consolas,sans-serif",
  "terminal.integrated.fontSize": 10,
  "prettier.embeddedLanguageFormatting": "off",
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": false,
  "prettier.resolveGlobalModules": false,
  "editor.tabSize": 2,
  "workbench.fontAliasing": "antialiased"
}
